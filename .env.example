# === Auth ===
# Secret used to encrypt session JWTs (generate with: openssl rand -base64 32)
AUTH_SECRET=

# === Single Registry Mode ===
# URL of the OCI registry (e.g. https://registry.example.com)
REGISTRY_URL=
# Optional: display name for the registry
REGISTRY_NAME=

# === Multi-Registry Mode ===
# Path to a JSON config file listing multiple registries
# When set, REGISTRY_URL and REGISTRY_NAME are ignored
# REGISTRIES_CONFIG_PATH=./registries.json

# === App ===
# Base URL of this app (used by Auth.js)
NEXTAUTH_URL=http://localhost:3000
